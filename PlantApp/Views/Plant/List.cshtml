@model PlantsListViewModel


@foreach (var plant in Model.Plants)
{
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="@plant.ImageUrl" alt="">
            <div class="caption">
                <h3>
                    <a>@plant.CommonName</a>
                </h3>
                <h4>@plant.LatinName</h4>
                <p>@plant.Description</p>
            </div>
        </div>
    </div>
}